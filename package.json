{
  "name": "skeleton-api-clean-code-basic",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "bin": "build/src/main.js",
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": [
      "build/**/*"
    ],
    "targets": [
      "node16"
    ]
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.config.ts",
    "prepare": "husky install",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/_core/@shared/infrastructure/adapter/persistence/typeorm/datasource.ts",
    "typeorm:migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create",
    "typeorm:migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert"
  },
  "dependencies": {
    "@faker-js/faker": "10.1.0",
    "@nestjs/common": "11.1.8",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.8",
    "@nestjs/platform-express": "11.1.8",
    "@nestjs/swagger": "11.2.1",
    "@nestjs/typeorm": "11.0.0",
    "@prisma/client": "6.19.0",
    "bcryptjs": "3.0.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "dotenv": "17.2.3",
    "dotenv-expand": "12.0.3",
    "env-var": "7.5.0",
    "helmet": "8.1.0",
    "joi": "18.0.1",
    "nestjs-pino": "4.4.1",
    "pg": "8.16.3",
    "pino-http": "11.0.0",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.1.0",
    "rxjs": "7.8.2",
    "swagger-themes": "1.4.3",
    "typeorm": "0.3.27",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@nestjs/cli": "11.0.11",
    "@nestjs/schematics": "11.0.9",
    "@nestjs/testing": "11.1.8",
    "@swc/core": "1.15.1",
    "@swc/jest": "0.2.39",
    "@types/express": "5.0.5",
    "@types/jest": "30.0.0",
    "@types/joi": "17.2.3",
    "@types/node": "24.10.0",
    "@types/pg": "8.15.6",
    "@types/supertest": "6.0.3",
    "@types/uuid": "11.0.0",
    "@typescript-eslint/eslint-plugin": "8.46.3",
    "@typescript-eslint/parser": "8.46.3",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "pino-pretty": "13.1.2",
    "prettier": "3.6.2",
    "prisma": "6.19.0",
    "source-map-support": "^0.5.20",
    "supertest": "7.1.4",
    "ts-jest": "29.4.5",
    "ts-loader": "9.5.4",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.3"
  },
  "overrides": {
    "nestjs-pino": {
      "pino-http": "^6.4.0 || ^7.0.0 || ^8.0.0 || ^9.0.0 || ^10.0.0 || ^11.0.0"
    }
  }
}
