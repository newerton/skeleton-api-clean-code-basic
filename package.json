{
  "name": "skeleton-api-clean-code-basic",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "bin": "build/src/main.js",
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": [
      "build/**/*"
    ],
    "targets": [
      "node16"
    ]
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.config.ts",
    "prepare": "husky install",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/_core/@shared/infrastructure/adapter/persistence/typeorm/datasource.ts",
    "typeorm:migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create",
    "typeorm:migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert"
  },
  "dependencies": {
    "@faker-js/faker": "7.6.0",
    "@nestjs/apollo": "10.1.7",
    "@nestjs/common": "9.2.1",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "10.1.7",
    "@nestjs/platform-express": "9.2.1",
    "@nestjs/swagger": "6.1.4",
    "@nestjs/typeorm": "9.0.1",
    "@prisma/client": "4.7.1",
    "apollo-server-express": "3.11.1",
    "apollo-server-plugin-base": "3.7.1",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "dotenv": "16.0.3",
    "dotenv-expand": "10.0.0",
    "env-var": "7.3.0",
    "graphql": "16.6.0",
    "graphql-query-complexity": "0.12.0",
    "helmet": "6.0.1",
    "joi": "17.7.0",
    "lightship": "7.1.1",
    "nestjs-pino": "3.1.1",
    "pg": "8.8.0",
    "pino-http": "8.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "7.8.0",
    "swagger-themes": "1.2.22",
    "typeorm": "0.3.11",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@nestjs/cli": "^9.3.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@swc/core": "1.3.23",
    "@swc/jest": "^0.2.24",
    "@types/express": "4.17.15",
    "@types/jest": "^29.2.4",
    "@types/joi": "17.2.3",
    "@types/node": "18.11.17",
    "@types/pg": "8.6.5",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "9.0.0",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "eslint": "8.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "8.0.2",
    "jest": "^29.3.1",
    "pino-pretty": "9.1.1",
    "prettier": "2.8.1",
    "prisma": "4.7.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  }
}
